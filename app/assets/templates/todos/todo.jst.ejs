<div class="todo <%= entry.get('completed') ? 'done' : '' %>">
  <div class="display">
    <input class="check" type="checkbox" <%= entry.get('completed') ? 'checked="checked"' : '' %> />
    <div class="todo-content"><%= entry.get('title') %></div>
    <span class="todo-destroy"></span>
  </div>
  <div class="edit">
    <input class="todo-input" type="text" value="" />
    <button>Edit</button>
  </div>
</div>